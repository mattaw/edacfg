# Cadence Allegro PCB 17.2
# Online documentation command: spbdoc
NAME SPB
VERSION 17.2
DESC Cadence Allegro PCB
ENV EDA_CFG_CDS_SPB ${EDA_CFG_ROOT}/cadence/SPB172
ENV CDS_INIT_DIR ${EDA_CFG_CDS_SPB}
APPENDIF PATH ${EDA_CFG_CDS_SPB}/tools/bin
APPENDIF PATH ${EDA_CFG_CDS_SPB}/tools/dfII/bin
APPENDIF CDS_LIC_FILE 1720@marina.ecn.purdue.edu
ENV CDS_AUTO_64BIT ALL
ALIAS spbdoc cdnshelp -hierarchy ${EDA_CFG_CDS_SPB}/doc
